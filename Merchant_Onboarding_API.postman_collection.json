				{
					"name": "Start Onboarding",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Onboarding started\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.status).to.eql('started');",
									"    pm.expect(jsonData).to.have.property('job_id');",
									"    pm.expect(jsonData).to.have.property('merchant_id');",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"merchant_id\": \"{{merchant_id}}\",\n    \"user_id\": \"{{user_id}}\",\n    \"shop_name\": \"My Awesome Store\",\n    \"shop_url\": \"https://example.myshopify.com\",\n    \"bot_name\": \"AI Shopping Assistant\",\n    \"target_customer\": \"Tech-savvy millennials interested in sustainable products\",\n    \"top_questions\": \"What are your return policies? How long does shipping take?\",\n    \"top_products\": \"Best sellers: Product A, Product B, Product C\",\n    \"primary_color\": \"#667eea\",\n    \"secondary_color\": \"#764ba2\",\n    \"logo_url\": \"https://example.com/logo.png\",\n    \"file_paths\": {\n        \"knowledge\": [\n            \"merchants/{{merchant_id}}/knowledge_base/knowledge-base.pdf\"\n        ],\n        \"products\": \"merchants/{{merchant_id}}/prompt-docs/products.csv\",\n        \"logo\": \"merchants/{{merchant_id}}/brand-images/logo.png\"\n    }\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{base_url}}/onboard",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"onboard"
							]
						},
