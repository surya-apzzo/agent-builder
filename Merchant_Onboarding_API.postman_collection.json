								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response has upload_url\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('upload_url');",
									"    pm.expect(jsonData).to.have.property('object_path');",
									"    ",
									"    // Save object_path, upload_url, and content_type for later use",
									"    pm.environment.set(\"knowledge_object_path\", jsonData.object_path);",
									"    pm.environment.set(\"knowledge_upload_url\", jsonData.upload_url);",
									"    // Use the Content-Type from the response headers (exact match required!)",
